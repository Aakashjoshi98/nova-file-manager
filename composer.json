{
    "name" : "bbs-lab/nova-file-manager",
    "description" : "A handy file manager tool for Laravel Nova",
    "keywords" : [
        "laravel",
        "nova",
        "file",
        "manager",
        "tool",
        "multiple",
        "filesystem",
        "disks"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "homepage": "https://github.com/BBS-Lab/nova-file-manager",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Charaf Rezrazi",
            "email" : "charaf@big-boss-studio.com"
        },
        {
            "name" : "MikaÃ«l Popowicz",
            "email" : "mpopowicz@big-boss-studio.com"
        }
    ],
    "require" : {
        "php" : "^8.0",
        "james-heinrich/getid3" : "^1.9",
        "pion/laravel-chunk-upload" : "^1.5",
        "laravel/nova" : "^4.0",
        "ext-json" : "*"
    },
    "require-dev" : {
        "laravel/pint": "^0.1.7",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.6",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload" : {
        "psr-4" : {
            "BBSLab\\NovaFileManager\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "BBSLab\\NovaFileManager\\Tests\\" : "tests/"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "BBSLab\\NovaFileManager\\ToolServiceProvider"
            ]
        }
    },
    "scripts" : {
        "pint" : [
            "./vendor/bin/pint"
        ],
        "test" : [
            "./vendor/bin/pest"
        ]
    },
    "config" : {
        "sort-packages" : true,
        "allow-plugins" : {
            "pestphp/pest-plugin" : true
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
